{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "armincms/json": "^0.5.2",
        "dive-be/nova-froala-field": "^1.1",
        "ebess/advanced-nova-media-library": "^4.2",
        "inertiajs/inertia-laravel": "^1.0",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.24",
        "laravel/jetstream": "^5.0",
        "laravel/nova": "^4.0",
        "laravel/pennant": "^1.7",
        "laravel/pulse": "^1.0@beta",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.8",
        "laravel/socialite": "^5.12",
        "laravel/telescope": "^5.0",
        "laravel/tinker": "^2.9",
        "maatwebsite/excel": "^3.1",
        "maatwebsite/laravel-nova-excel": "^1.3",
        "outl1ne/nova-settings": "^5.2",
        "sereny/nova-permissions": "^1.6",
        "spatie/calendar-links": "^1.11",
        "spatie/laravel-medialibrary": "^11.4",
        "spatie/laravel-translatable": "^6.6",
        "spatie/nova-translatable": "^4.0",
        "stepanenko3/nova-json": "^4.0",
        "tightenco/ziggy": "^2.0",
        "whitecube/nova-page": "^0.2.8"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.15",
        "laravel/sail": "^1.26",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0.1",
        "spatie/laravel-ignition": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "beta",
    "prefer-stable": true
}
